generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id            String    @id @default(cuid())
  name          String?
  email         String?   @unique
  emailVerified DateTime? @map("email_verified")
  image         String?
  password      String?
  isFormFilled   Boolean @default(false)
  @@map("users")
}

model VerificationToken{
  id String @id @default(cuid())
  email String
  token String
  expires DateTime

  @@unique([email,token])
}